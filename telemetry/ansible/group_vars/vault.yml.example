---
# Sensitive variables for Basilica telemetry deployment
# Encrypt this file with: ansible-vault encrypt group_vars/vault.yml

# Grafana credentials
vault_grafana_admin_password: "your_secure_password_here"
vault_grafana_secret_key: "your_grafana_secret_key_here"

# SSL Certificate passwords (if using encrypted certificates)
vault_ssl_cert_password: "ssl_cert_password_here"

# Database credentials (if using external databases)
vault_prometheus_remote_write_password: "prometheus_password_here"
vault_loki_storage_access_key: "storage_access_key_here"
vault_loki_storage_secret_key: "storage_secret_key_here"

